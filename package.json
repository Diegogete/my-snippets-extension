{
  "name": "FrontedSnippets",
  "displayName": "Fronted Snippets",
  "version": "1.0.0",
  "description": "Uma coleção de snippets personalizados",
  "publisher": "FrontedSnippets",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "html",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Diegogete/my-snippets-extension"
  }
}